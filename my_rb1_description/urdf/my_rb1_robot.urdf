<?xml version="1.0" ?>
<robot name="rb1_robot">

    <!-- Gazebo Color Tags -->
    <gazebo reference="base_link">
        <visual>
            <material> <!-- Black -->
                <ambient>0.1 0.1 0.1 1.0</ambient>
                <diffuse>0.1 0.1 0.1 1.0</diffuse>
                <specular>0.0 0.0 0.0 1.0</specular>
                <emissive>0.0 0.0 0.0 1.0</emissive>
            </material>
        </visual>
    </gazebo>

    <!-- Gazebo friction and material properties for left_wheel -->
    <gazebo reference="left_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Gazebo friction and material properties for right_wheel -->
    <gazebo reference="right_wheel">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>1.0</mu>
                        <mu2>1.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Gazebo friction and material properties for front_caster -->
    <gazebo reference="front_caster">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>
                        <mu2>0.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Gazebo friction and material properties for back_caster -->
    <gazebo reference="back_caster">
        <collision>
            <surface>
                <friction>
                    <ode>
                        <mu>0.0</mu>
                        <mu2>0.0</mu2>
                    </ode>
                </friction>
            </surface>
        </collision>
    </gazebo>

    <!-- Define materials for visualization -->
    <material name="blue">
        <color rgba="0 0 1 1"/>
    </material>

    <material name="grey">
        <color rgba="0.40 0.40 0.40 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <!-- Base footprint link -->
    <link name="base_footprint">
    </link>

    <!-- Joint connecting base_link and base_footprint -->
    <joint name="base_link_plate_joint" type="fixed">
        <parent link="base_footprint"/>
        <child link="base_link"/>
        <origin xyz="0 0 0.15" rpy="0 0 0"/>
    </joint>

    <!-- Base link -->
    <link name="base_link">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="23.5"/>
            <inertia ixx="0.5434375" ixy="0" ixz="0" iyy="0.5434375" iyz="0" izz="0.734375"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
            <material name="Black"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.25" length="0.30"/>
            </geometry>
        </collision>
    </link>

    <!-- Left wheel link -->
    <link name="left_wheel">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.4"/>
            <inertia ixx="7.583333333333335e-05" ixy="0" ixz="0" iyy="0.000125" iyz="0" izz="7.583333333333335e-05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
            <material name="red"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
            <geometry>
                <cylinder radius="0.025" length="0.02"/>
            </geometry>
        </collision>
    </link>

    <!-- Joint connecting base_link and left_wheel -->
    <joint name="base_link_left_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- Right wheel link -->
    <link name="right_wheel">
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="0.4"/>
      <inertia ixx="7.583333333333335e-05" ixy="0" ixz="0" iyy="0.000125" iyz="0" izz="7.583333333333335e-05"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
      <geometry>
        <cylinder radius="0.025" length="0.02"/>
      </geometry>
      <material name="red"/>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57079632679 0 0"/>
      <geometry>
        <cylinder radius="0.025" length="0.02"/>
      </geometry>
    </collision>
    </link>

    <!-- Joint connecting base_link and right_wheel -->
    <joint name="base_link_right_wheel_joint" type="continuous">
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/>
        <axis xyz="0 1 0"/>
    </joint>

    <!-- Front caster -->
    <link name="front_caster">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="2.5e-05" ixy="0" ixz="0" iyy="2.5e-05" iyz="0" izz="2.5e-05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="grey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <gazebo reference="front_caster">
                <mu1>0.0</mu1>
                <mu2>0.0</mu2>
            </gazebo>
        </collision>
    </link>

    <!-- Joint connecting base_link and front_caster -->
    <joint name="base_link_front_caster_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_caster"/>
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
    </joint>

    <!-- Back caster -->
    <link name="back_caster">
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.1"/>
            <inertia ixx="2.5e-05" ixy="0" ixz="0" iyy="2.5e-05" iyz="0" izz="2.5e-05"/>
        </inertial>
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="grey"/>
        </visual>
        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <gazebo reference="back_caster">
                <mu1>0.0</mu1>
                <mu2>0.0</mu2>
            </gazebo>
        </collision>
    </link>

    <!-- Joint connecting base_link and back_caster -->
    <joint name="base_link_back_caster_joint" type="fixed">
        <parent link="base_link"/>
        <child link="back_caster"/>
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
    </joint>

    <!-- Link representing the front laser scanner -->
    <!--For this robot, the laser is not visible, so we won't define a body for it. -->
    <link name="front_laser">
    </link>

    <!-- Joint connecting base_link and front_laser -->
    <joint name="base_link_front_laser_joint" type="fixed">
        <parent link="base_link"/>
        <child link="front_laser"/>
        <origin xyz="0.25 0.0 0.075" rpy="3.14159265359 0 0"/>
    </joint>

    <!-- Differential Drive Plugin -->
    <gazebo>
        <plugin 
            filename="libignition-gazebo-diff-drive-system.so"
            name="ignition::gazebo::systems::DiffDrive">
            <left_joint>base_link_left_wheel_joint</left_joint>
            <right_joint>base_link_right_wheel_joint</right_joint>
            <wheel_separation>0.4</wheel_separation>
            <wheel_radius>0.025</wheel_radius>
            <odom_publish_frequency>20</odom_publish_frequency>
            <topic>cmd_vel</topic>
            <odom_topic>odom</odom_topic>
            <tf_topic>tf</tf_topic>
            <frame_id>odom</frame_id>
            <child_frame_id>base_link</child_frame_id>
        </plugin>
    </gazebo>

</robot>